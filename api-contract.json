          "customerId": { "type": "string", "format": "uuid" }
        },
        "required": ["eventId", "title", "eventDate", "venueId", "eventTypeId", "customerId"]
      },
      "GetEventsResponse": {
        "type": "object",
        "properties": {
          "events": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/EventDto" }
          },
          "totalCount": { "type": "integer" },
          "page": { "type": "integer" },
          "pageSize": { "type": "integer" }
        },
        "required": ["events", "totalCount", "page", "pageSize"]
      },
      "GetEventByIdResponse": {
        "type": "object",
        "properties": {
          "event": { "$ref": "#/components/schemas/EventDto" }
        },
        "required": ["event"]
      },
      "CreateEventResponse": {
        "type": "object",
        "properties": {
          "event": { "$ref": "#/components/schemas/EventDto" }
        },
        "required": ["event"]
      },
      "UpdateEventResponse": {
        "type": "object",
        "properties": {
          "event": { "$ref": "#/components/schemas/EventDto" }
        },
        "required": ["event"]
      },
      "UpdateEventStatusResponse": {
        "type": "object",
        "properties": {
          "event": { "$ref": "#/components/schemas/EventDto" }
        },
        "required": ["event"]
      },
      "StaffStatus": {
        "type": "string",
        "enum": ["Active", "OnLeave", "Inactive", "Terminated"]
      },
      "StaffRole": {
        "type": "string",
        "enum": ["Manager", "Coordinator", "Technician", "Security", "Catering", "Cleaning", "Driver", "Other"]
      },
      "StaffMemberDto": {
        "type": "object",
        "properties": {
          "staffMemberId": { "type": "string", "format": "uuid" },
          "firstName": { "type": "string" },
          "lastName": { "type": "string" },
          "email": { "type": "string", "format": "email" },
          "phoneNumber": { "type": "string" },
          "photoUrl": { "type": "string", "nullable": true },
          "status": { "$ref": "#/components/schemas/StaffStatus" },
          "hireDate": { "type": "string", "format": "date-time" },
          "terminationDate": { "type": "string", "format": "date-time", "nullable": true },
          "role": { "$ref": "#/components/schemas/StaffRole" },
          "hourlyRate": { "type": "number", "nullable": true },
          "createdAt": { "type": "string", "format": "date-time" },
          "modifiedAt": { "type": "string", "format": "date-time", "nullable": true }
        },
        "required": ["staffMemberId", "firstName", "lastName", "email", "phoneNumber", "status", "hireDate", "role", "createdAt"]
      },
      "CreateStaffMemberRequest": {
        "type": "object",
        "properties": {
          "firstName": { "type": "string" },
          "lastName": { "type": "string" },
          "email": { "type": "string", "format": "email" },
          "phoneNumber": { "type": "string" },
          "hireDate": { "type": "string", "format": "date-time" },
          "role": { "$ref": "#/components/schemas/StaffRole" },
          "hourlyRate": { "type": "number", "nullable": true }
        },
        "required": ["firstName", "lastName", "email", "phoneNumber", "hireDate", "role"]
      },
      "UpdateStaffMemberRequest": {
        "type": "object",
        "properties": {
          "staffMemberId": { "type": "string", "format": "uuid" },
          "firstName": { "type": "string" },
          "lastName": { "type": "string" },
          "phoneNumber": { "type": "string" },
          "role": { "$ref": "#/components/schemas/StaffRole" },
          "hourlyRate": { "type": "number", "nullable": true }
        },
        "required": ["staffMemberId", "firstName", "lastName", "phoneNumber", "role"]
      },
      "GetStaffMembersResponse": {
        "type": "object",
        "properties": {
          "staffMembers": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/StaffMemberDto" }
          },
          "totalCount": { "type": "integer" },
          "page": { "type": "integer" },
          "pageSize": { "type": "integer" }
        },
        "required": ["staffMembers", "totalCount", "page", "pageSize"]
      },
      "GetStaffMemberByIdResponse": {
        "type": "object",
        "properties": {
          "staffMember": { "$ref": "#/components/schemas/StaffMemberDto" }
        },
        "required": ["staffMember"]
      },
      "CreateStaffMemberResponse": {
        "type": "object",
        "properties": {
          "staffMember": { "$ref": "#/components/schemas/StaffMemberDto" }
        },
        "required": ["staffMember"]
      },
      "UpdateStaffMemberResponse": {
        "type": "object",
        "properties": {
          "staffMember": { "$ref": "#/components/schemas/StaffMemberDto" }
        },
        "required": ["staffMember"]
      },
      "EquipmentCategory": {
        "type": "string",
