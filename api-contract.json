      },
      "EquipmentCondition": {
        "type": "string",
        "enum": ["New", "Excellent", "Good", "Fair", "Poor", "NeedsRepair"]
      },
      "EquipmentStatus": {
        "type": "string",
        "enum": ["Available", "InUse", "Reserved", "UnderMaintenance", "Retired"]
      },
      "EquipmentItemDto": {
        "type": "object",
        "properties": {
          "equipmentItemId": { "type": "string", "format": "uuid" },
          "name": { "type": "string" },
          "description": { "type": "string", "nullable": true },
          "category": { "$ref": "#/components/schemas/EquipmentCategory" },
          "condition": { "$ref": "#/components/schemas/EquipmentCondition" },
          "status": { "$ref": "#/components/schemas/EquipmentStatus" },
          "purchaseDate": { "type": "string", "format": "date-time" },
          "purchasePrice": { "type": "number" },
          "currentValue": { "type": "number", "nullable": true },
          "manufacturer": { "type": "string", "nullable": true },
          "model": { "type": "string", "nullable": true },
          "serialNumber": { "type": "string", "nullable": true },
          "warehouseLocation": { "type": "string", "nullable": true },
          "isActive": { "type": "boolean" },
          "createdAt": { "type": "string", "format": "date-time" },
          "modifiedAt": { "type": "string", "format": "date-time", "nullable": true }
