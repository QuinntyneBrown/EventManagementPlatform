{
  "openapi": "3.0.0",
  "info": {
    "title": "Event Management Platform API",
    "version": "1.0.0",
    "description": "API contract for the Event Management Platform"
  },
  "servers": [
    {
      "url": "/api",
      "description": "API Base URL"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "CustomerStatus": {
        "type": "string",
        "enum": ["Active", "Inactive", "Pending", "Blocked"]
      },
      "CustomerType": {
        "type": "string",
        "enum": ["Individual", "Corporate", "NonProfit", "Government"]
      },
      "CustomerDto": {
        "type": "object",
        "properties": {
          "customerId": { "type": "string", "format": "uuid" },
          "companyName": { "type": "string" },
          "type": { "$ref": "#/components/schemas/CustomerType" },
          "primaryEmail": { "type": "string", "format": "email" },
          "primaryPhone": { "type": "string" },
          "industry": { "type": "string", "nullable": true },
          "website": { "type": "string", "nullable": true },
          "billingStreet": { "type": "string" },
          "billingCity": { "type": "string" },
          "billingState": { "type": "string" },
          "billingPostalCode": { "type": "string" },
          "billingCountry": { "type": "string" },
          "status": { "$ref": "#/components/schemas/CustomerStatus" },
          "createdAt": { "type": "string", "format": "date-time" },
          "modifiedAt": { "type": "string", "format": "date-time", "nullable": true }
        },
        "required": ["customerId", "companyName", "type", "primaryEmail", "primaryPhone", "billingStreet", "billingCity", "billingState", "billingPostalCode", "billingCountry", "status", "createdAt"]
      },
      "CreateCustomerRequest": {
        "type": "object",
        "properties": {
          "company": { "type": "string" },
          "type": { "$ref": "#/components/schemas/CustomerType" },
          "email": { "type": "string", "format": "email" },
          "phone": { "type": "string" },
          "industry": { "type": "string", "nullable": true },
          "website": { "type": "string", "nullable": true },
          "address": { "type": "string" },
          "city": { "type": "string" },
          "state": { "type": "string" },
          "postalCode": { "type": "string" },
          "billingCountry": { "type": "string", "default": "Canada" }
        },
        "required": ["company", "type", "email", "phone", "address", "city", "state", "postalCode"]
      },
      "UpdateCustomerRequest": {
        "type": "object",
        "properties": {
          "customerId": { "type": "string", "format": "uuid" },
          "companyName": { "type": "string" },
          "type": { "$ref": "#/components/schemas/CustomerType" },
          "primaryEmail": { "type": "string", "format": "email" },
          "primaryPhone": { "type": "string" },
          "industry": { "type": "string", "nullable": true },
          "website": { "type": "string", "nullable": true },
          "billingStreet": { "type": "string" },
          "billingCity": { "type": "string" },
          "billingState": { "type": "string" },
          "billingPostalCode": { "type": "string" },
          "billingCountry": { "type": "string" }
        },
        "required": ["customerId", "companyName", "type", "primaryEmail", "primaryPhone", "billingStreet", "billingCity", "billingState", "billingPostalCode", "billingCountry"]
      },
      "GetCustomersResponse": {
        "type": "object",
        "properties": {
          "customers": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/CustomerDto" }
          },
          "totalCount": { "type": "integer" },
          "page": { "type": "integer" },
          "pageSize": { "type": "integer" }
        },
        "required": ["customers", "totalCount", "page", "pageSize"]
      },
      "GetCustomerByIdResponse": {
        "type": "object",
        "properties": {
          "customer": { "$ref": "#/components/schemas/CustomerDto" }
        },
        "required": ["customer"]
      },
      "CreateCustomerResponse": {
        "type": "object",
        "properties": {
          "customer": { "$ref": "#/components/schemas/CustomerDto" }
        },
        "required": ["customer"]
      },
      "UpdateCustomerResponse": {
        "type": "object",
        "properties": {
          "customer": { "$ref": "#/components/schemas/CustomerDto" }
        },
        "required": ["customer"]
      },
      "VenueStatus": {
        "type": "string",
        "enum": ["Active", "Inactive", "UnderMaintenance"]
      },
      "VenueType": {
        "type": "string",
        "enum": ["ConferenceCenter", "BanquetHall", "Hotel", "Outdoor", "Theater", "Restaurant", "Museum", "Other"]
      },
      "VenueDto": {
        "type": "object",
